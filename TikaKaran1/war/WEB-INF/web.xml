<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
<servlet>
		<servlet-name>Final</servlet-name>
		<servlet-class>com.vaccine.Controller.SignupController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Final</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>childLogin</servlet-name>
		<servlet-class>com.vaccine.Controller.LoginChildController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>childLogin</servlet-name>
		<url-pattern>/loginChild</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>com.vaccine.Controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Forgot</servlet-name>
		<servlet-class>com.vaccine.Controller.ForgotPasswordController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Forgot</servlet-name>
		<url-pattern>/forgot</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Feedback3</servlet-name>
		<servlet-class>com.vaccine.Controller.Feedback3Controller</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Feedback3</servlet-name>
		<url-pattern>/sendFeedback</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AskQuestion</servlet-name>
		<servlet-class>com.vaccine.Controller.AskQuestionController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AskQuestion</servlet-name>
		<url-pattern>/AskQuestion</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>googlesignin</servlet-name>
		<servlet-class>com.vaccine.Controller.GoogleSignInController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>googlesignin</servlet-name>
		<url-pattern>/gsigninmobno</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>googlesignin1</servlet-name>
		<servlet-class>com.vaccine.Controller.GoogleSignInController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>googlesignin1</servlet-name>
		<url-pattern>/googlesignin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>googlesignin2</servlet-name>
		<servlet-class>com.vaccine.Controller.GoogleDetailsController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>googlesignin2</servlet-name>
		<url-pattern>/gsignin</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Image_demo</servlet-name>
		<servlet-class>com.vaccine.Controller.UploadController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Image_demo</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>serve</servlet-name>
		<jsp-file>serve.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>serve</servlet-name>
		<url-pattern>/serve</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>dashboard_jsp</servlet-name>
		<jsp-file>dashboard.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>dashboard_jsp</servlet-name>
		<url-pattern>/dashboard_jsp</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout> 20 </session-timeout>
	</session-config>
	
	 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
 
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
